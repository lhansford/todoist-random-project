{
  "name": "todoist-random-project",
  "version": "1.1.1",
  "description": "Connects to your Todoist account and returns a random project",
  "main": "dist/index.js",
  "type": "module",
  "author": "Luke Hansford",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config ./config/rollup.config.js",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --ext .js,.ts --fix",
    "start": "npm run build && node dist/index.js",
    "test": "jest --config ./config/jest.config.js",
    "test:watch": "jest --watchAll",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^27.0.2",
    "@types/node": "^20.12.7",
    "babel-jest": "^27.2.4",
    "eslint": "^7.32.0",
    "eslint-config-lukehansford-base": "^1.4.1",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "rollup": "^2.58.0",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": "20.x"
  }
}
